00787 {\cf18 void} MainWindow::pasteFromClipboard()\par
00788 \{\par
00789     QTableView* tableView = getCurrentTableView();\par
00790     {\cf19 if} (!tableView)\par
00791         {\cf19 return};\par
00792 \par
00793     {\cf20 // \'CF\'EE\'EB\'F3\'F7\'E0\'E5\'EC \'E4\'E0\'ED\'ED\'FB\'E5 \'E8\'E7 \'E1\'F3\'F4\'E5\'F0\'E0 \'EE\'E1\'EC\'E5\'ED\'E0}\par
00794     {\cf17 const} QMimeData* mimeData = QApplication::clipboard()->mimeData();\par
00795     {\cf19 if} (!mimeData->hasText())\par
00796         {\cf19 return};\par
00797 \par
00798     {\cf20 // \'CF\'EE\'EB\'F3\'F7\'E0\'E5\'EC \'F2\'E5\'EA\'F3\'F9\'E8\'E9 \'E8\'ED\'E4\'E5\'EA\'F1, \'EA\'F3\'E4\'E0 \'E1\'F3\'E4\'E5\'EC \'E2\'F1\'F2\'E0\'E2\'EB\'FF\'F2\'FC \'E4\'E0\'ED\'ED\'FB\'E5}\par
00799     QModelIndex currentIndex = tableView->currentIndex();\par
00800     {\cf19 if} (!currentIndex.isValid()) \{\par
00801         {\cf20 // \'C5\'F1\'EB\'E8 \'ED\'E5\'F2 \'F2\'E5\'EA\'F3\'F9\'E5\'E3\'EE \'E8\'ED\'E4\'E5\'EA\'F1\'E0, \'E8\'F1\'EF\'EE\'EB\'FC\'E7\'F3\'E5\'EC \'E2\'E5\'F0\'F5\'ED\'E8\'E9 \'EB\'E5\'E2\'FB\'E9 \'F3\'E3\'EE\'EB}\par
00802         currentIndex = tableView->model()->index(0, 0);\par
00803     \}\par
00804 \par
00805     {\cf18 int} startRow = currentIndex.row();\par
00806     {\cf18 int} startCol = currentIndex.column();\par
00807 \par
00808     {\cf20 // \'CF\'E0\'F0\'F1\'E8\'EC CSV}\par
00809     QString csv = mimeData->text();\par
00810     QStringList rows = csv.split({\cf23 '\\n'}, Qt::SkipEmptyParts);\par
00811     \par
00812     {\cf20 // \'D1\'EE\'E7\'E4\'E0\'E5\'EC \'EC\'EE\'E4\'E8\'F4\'E8\'F6\'E8\'F0\'EE\'E2\'E0\'ED\'ED\'FB\'E5 \'E7\'E0\'EF\'E8\'F1\'E8}\par
00813     {\cf19 for} ({\cf18 int} i = 0; i < rows.size(); ++i) \{\par
00814         QString row = rows[i];\par
00815         QStringList columns;\par
00816         \par
00817         {\cf20 // \'CA\'EE\'F0\'F0\'E5\'EA\'F2\'ED\'EE \'EE\'E1\'F0\'E0\'E1\'E0\'F2\'FB\'E2\'E0\'E5\'EC CSV \'F1 \'F3\'F7\'E5\'F2\'EE\'EC \'EA\'E0\'E2\'FB\'F7\'E5\'EA}\par
00818         {\cf18 bool} inQuote = {\cf17 false};\par
00819         {\cf18 int} quoteStart = -1;\par
00820         QString col;\par
00821         \par
00822         {\cf19 for} ({\cf18 int} j = 0; j < row.size(); ++j) \{\par
00823             {\cf17 const} QChar c = row[j];\par
00824             \par
00825             {\cf19 if} (c == {\cf23 '"'}) \{\par
00826                 {\cf19 if} (inQuote) \{\par
00827                     {\cf20 // \'CF\'F0\'EE\'E2\'E5\'F0\'FF\'E5\'EC \'E4\'E2\'EE\'E9\'ED\'FB\'E5 \'EA\'E0\'E2\'FB\'F7\'EA\'E8}\par
00828                     {\cf19 if} (j + 1 < row.size() && row[j + 1] == {\cf23 '"'}) \{\par
00829                         col += {\cf23 '"'};\par
00830                         j++; {\cf20 // \'CF\'F0\'EE\'EF\'F3\'F1\'EA\'E0\'E5\'EC \'F1\'EB\'E5\'E4\'F3\'FE\'F9\'F3\'FE \'EA\'E0\'E2\'FB\'F7\'EA\'F3}\par
00831                     \} {\cf19 else} \{\par
00832                         inQuote = {\cf17 false};\par
00833                     \}\par
00834                 \} {\cf19 else} \{\par
00835                     inQuote = {\cf17 true};\par
00836                     quoteStart = j;\par
00837                 \}\par
00838             \} {\cf19 else} {\cf19 if} (c == {\cf23 ','} && !inQuote) \{\par
00839                 columns.append(col);\par
00840                 col.clear();\par
00841             \} {\cf19 else} \{\par
00842                 col += c;\par
00843             \}\par
00844         \}\par
00845         \par
00846         {\cf19 if} (!col.isEmpty()) \{\par
00847             columns.append(col);\par
00848         \}\par
00849         \par
00850         {\cf20 // \'CE\'E1\'ED\'EE\'E2\'EB\'FF\'E5\'EC \'E4\'E0\'ED\'ED\'FB\'E5 \'EC\'EE\'E4\'E5\'EB\'E8}\par
00851         {\cf19 for} ({\cf18 int} j = 0; j < columns.size(); ++j) \{\par
00852             {\cf18 int} row = startRow + i;\par
00853             {\cf18 int} col = startCol + j;\par
00854             \par
00855             {\cf19 if} (row < tableView->model()->rowCount() && col < tableView->model()->columnCount()) \{\par
00856                 QModelIndex idx = tableView->model()->index(row, col);\par
00857                 tableView->model()->setData(idx, columns[j], Qt::EditRole);\par
00858             \}\par
00859         \}\par
00860     \}\par
00861     \par
00862     statusBar()->showMessage(tr({\cf22 "Data pasted from clipboard"}), 3000);\par
00863 \}\par
