00026     \}\par
00027 \par
00028     ui->comboBox->addItem(tr({\cf22 "Equipment by Type"}));\par
00029     ui->comboBox->addItem(tr({\cf22 "Price Distribution"}));\par
00030     ui->comboBox->addItem(tr({\cf22 "Location Statistics"}));\par
00031 \par
00032     updateChart(0);\par
00033 \}\par
00034 \par
00035 chartwindow::~chartwindow() \{\par
00036     {\cf17 delete} ui;\par
00037 \}\par
