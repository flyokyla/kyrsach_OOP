00982     {\cf20 // \'D1\'EE\'E7\'E4\'E0\'E5\'EC \'EA\'E0\'F0\'F2\'F3 \'F1\'EE\'EE\'F2\'E2\'E5\'F2\'F1\'F2\'E2\'E8\'FF \'E7\'E0\'E3\'EE\'EB\'EE\'E2\'EA\'EE\'E2 \'E8 \'E8\'ED\'E4\'E5\'EA\'F1\'EE\'E2 \'EA\'EE\'EB\'EE\'ED\'EE\'EA}\par
00983     QMap<int, int> columnMap;\par
00984     {\cf19 for} ({\cf18 int} i = 0; i < headers.size(); ++i) \{\par
00985         QString header = headers[i];\par
00986         {\cf19 for} ({\cf18 int} j = 0; j < model->columnCount(); ++j) \{\par
00987             {\cf19 if} (model->headerData(j, Qt::Horizontal).toString() == header) \{\par
00988                 columnMap[i] = j;\par
00989                 {\cf19 break};\par
00990             \}\par
00991         \}\par
00992     \}\par
00993 \par
00994     {\cf20 // \'C5\'F1\'EB\'E8 \'ED\'E5 \'F3\'E4\'E0\'EB\'EE\'F1\'FC \'ED\'E0\'E9\'F2\'E8 \'F1\'EE\'EE\'F2\'E2\'E5\'F2\'F1\'F2\'E2\'E8\'FF \'E4\'EB\'FF \'E7\'E0\'E3\'EE\'EB\'EE\'E2\'EA\'EE\'E2, \'EF\'F0\'E5\'E4\'F3\'EF\'F0\'E5\'E6\'E4\'E0\'E5\'EC}\par
00995     {\cf19 if} (columnMap.isEmpty()) \{\par
00996         QMessageBox msgBox({\cf17 this});\par
00997         msgBox.setWindowTitle(tr({\cf22 "Warning"}));\par
00998         msgBox.setText(tr({\cf22 "Could not match CSV headers with table columns"}));\par
00999         msgBox.setIcon(QMessageBox::Warning);\par
01000         msgBox.addButton(QCoreApplication::translate({\cf22 "QPlatformTheme"}, {\cf22 "OK"}), QMessageBox::AcceptRole);\par
01001         msgBox.exec();\par
01002         file.close();\par
01003         {\cf19 return};\par
01004     \}\par
01005 \par
01006     {\cf20 // \'D7\'E8\'F2\'E0\'E5\'EC \'E4\'E0\'ED\'ED\'FB\'E5 \'E8\'E7 CSV \'E8 \'E4\'EE\'E1\'E0\'E2\'EB\'FF\'E5\'EC \'E8\'F5 \'E2 \'EC\'EE\'E4\'E5\'EB\'FC}\par
01007     {\cf18 int} imported = 0;\par
01008     {\cf19 while} (!stream.atEnd()) \{\par
01009         QString line = stream.readLine();\par
01010         {\cf19 if} (line.trimmed().isEmpty())\par
01011             {\cf19 continue};\par
01012 \par
01013         {\cf20 // \'CF\'E0\'F0\'F1\'E8\'EC CSV \'F1 \'F3\'F7\'E5\'F2\'EE\'EC \'EA\'E0\'E2\'FB\'F7\'E5\'EA}\par
01014         QStringList fields;\par
01015         {\cf18 bool} inQuote = {\cf17 false};\par
01016         {\cf18 int} quoteStart = -1;\par
01017         QString field;\par
01018         \par
01019         {\cf19 for} ({\cf18 int} i = 0; i < line.size(); ++i) \{\par
01020             {\cf17 const} QChar c = line[i];\par
01021             \par
01022             {\cf19 if} (c == {\cf23 '"'}) \{\par
01023                 {\cf19 if} (inQuote) \{\par
01024                     {\cf20 // \'CF\'F0\'EE\'E2\'E5\'F0\'FF\'E5\'EC \'E4\'E2\'EE\'E9\'ED\'FB\'E5 \'EA\'E0\'E2\'FB\'F7\'EA\'E8}\par
01025                     {\cf19 if} (i + 1 < line.size() && line[i + 1] == {\cf23 '"'}) \{\par
01026                         field += {\cf23 '"'};\par
01027                         i++; {\cf20 // \'CF\'F0\'EE\'EF\'F3\'F1\'EA\'E0\'E5\'EC \'F1\'EB\'E5\'E4\'F3\'FE\'F9\'F3\'FE \'EA\'E0\'E2\'FB\'F7\'EA\'F3}\par
01028                     \} {\cf19 else} \{\par
01029                         inQuote = {\cf17 false};\par
01030                     \}\par
01031                 \} {\cf19 else} \{\par
01032                     inQuote = {\cf17 true};\par
01033                     quoteStart = i;\par
01034                 \}\par
01035             \} {\cf19 else} {\cf19 if} (c == {\cf23 ','} && !inQuote) \{\par
01036                 fields.append(field);\par
01037                 field.clear();\par
01038             \} {\cf19 else} \{\par
01039                 field += c;\par
01040             \}\par
01041         \}\par
01042         \par
01043         {\cf19 if} (!field.isEmpty()) \{\par
01044             fields.append(field);\par
01045         \}\par
01046 \par
01047         {\cf20 // \'D1\'EE\'E7\'E4\'E0\'E5\'EC \'ED\'EE\'E2\'F3\'FE \'E7\'E0\'EF\'E8\'F1\'FC \'E8 \'E7\'E0\'EF\'EE\'EB\'ED\'FF\'E5\'EC \'E4\'E0\'ED\'ED\'FB\'EC\'E8 \'E8\'E7 CSV}\par
01048         EquipmentRecord record;\par
01049         {\cf19 for} ({\cf18 int} i = 0; i < fields.size() && i < headers.size(); ++i) \{\par
01050             {\cf19 if} (columnMap.contains(i)) \{\par
01051                 {\cf18 int} modelColumn = columnMap[i];\par
01052                 QString value = fields[i];\par
01053                 \par
01054                 {\cf20 // \'D3\'F1\'F2\'E0\'ED\'EE\'E2\'EA\'E0 \'E7\'ED\'E0\'F7\'E5\'ED\'E8\'E9 \'E2 \'E7\'E0\'EF\'E8\'F1\'FC \'E2 \'E7\'E0\'E2\'E8\'F1\'E8\'EC\'EE\'F1\'F2\'E8 \'EE\'F2 \'EA\'EE\'EB\'EE\'ED\'EA\'E8}\par
01055                 {\cf19 switch} (modelColumn) \{\par
01056                     {\cf19 case} 0: record.id = value.trimmed(); {\cf19 break};\par
01057                     {\cf19 case} 1: record.type = value.trimmed(); {\cf19 break};\par
01058                     {\cf19 case} 2: record.model = value.trimmed(); {\cf19 break};\par
01059                     {\cf19 case} 3: record.serial = value.trimmed(); {\cf19 break};\par
01060                     {\cf19 case} 4: record.purchase_date = QDate::fromString(value.trimmed(), Qt::ISODate); {\cf19 break};\par
01061                     {\cf19 case} 5: record.price = value.trimmed().toDouble(); {\cf19 break};\par
01062                     {\cf19 case} 6: record.location = value.trimmed(); {\cf19 break};\par
01063                     {\cf19 case} 7: record.status = value.trimmed(); {\cf19 break};\par
01064                     {\cf19 case} 8: record.notes = value.trimmed(); {\cf19 break};\par
01065                     {\cf19 default}: {\cf19 break};\par
01066                 \}\par
01067             \}\par
01068         \}\par
01069 \par
01070         {\cf20 // \'CF\'F0\'EE\'E2\'E5\'F0\'FF\'E5\'EC \'E2\'E0\'EB\'E8\'E4\'ED\'EE\'F1\'F2\'FC \'E7\'E0\'EF\'E8\'F1\'E8 \'E8 \'E4\'EE\'E1\'E0\'E2\'EB\'FF\'E5\'EC \'E5\'E5 \'E2 \'EC\'EE\'E4\'E5\'EB\'FC}\par
01071         {\cf19 if} (record.isValid() && !model->hasEquipmentId(record.id)) \{\par
01072             model->addRecord(record);\par
01073             imported++;\par
01074         \}\par
01075     \}\par
01076 \par
01077     file.close();\par
01078     statusBar()->showMessage(tr({\cf22 "Imported %1 records from %2"}).arg(imported).arg(fileName), 3000);\par
01079 \}\par
01080 \par
01081 QTableView* MainWindow::getCurrentTableView(){\cf17  const }\{\par
01082     TabData *tab = getCurrentTab();\par
01083     {\cf19 if} (tab) \{\par
01084         {\cf19 return} tab->tableView;\par
01085     \}\par
01086     {\cf19 return} {\cf17 nullptr};\par
01087 \}\par
01088 \par
01089 EquipmentModel* MainWindow::getCurrentModel(){\cf17  const }\{\par
01090     TabData *tab = getCurrentTab();\par
01091     {\cf19 if} (tab) \{\par
01092         {\cf19 return} tab->model;\par
01093     \}\par
01094     {\cf19 return} {\cf17 nullptr};\par
01095 \}\par
}
