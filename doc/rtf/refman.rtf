00139 {\cf18 bool} EquipmentModel::dropMimeData({\cf17 const} QMimeData *data, Qt::DropAction action,\par
00140                                  {\cf18 int} row, {\cf18 int} column, {\cf17 const} QModelIndex &parent) \{\par
00141     {\cf19 if} (action == Qt::IgnoreAction)\par
00142         {\cf19 return} {\cf17 true};\par
00143     \par
00144     {\cf20 // \'C5\'F1\'EB\'E8 \'FD\'F2\'EE \'E2\'ED\'F3\'F2\'F0\'E5\'ED\'ED\'E5\'E5 \'EF\'E5\'F0\'E5\'F2\'E0\'F1\'EA\'E8\'E2\'E0\'ED\'E8\'E5 (\'EC\'E5\'E6\'E4\'F3 \'F2\'E0\'E1\'E0\'EC\'E8)}\par
00145     {\cf19 if} (data->hasFormat({\cf22 "application/vnd.text.list"})) \{\par
00146         QByteArray encodedData = data->data({\cf22 "application/vnd.text.list"});\par
00147         QDataStream stream(&encodedData, QIODevice::ReadOnly);\par
00148         \par
00149         QMap<int, QMap<int, QVariant>> cellData;\par
00150         stream >> cellData;\par
00151         \par
00152         {\cf18 int} targetRow = row;\par
00153         {\cf19 if} (targetRow < 0) \{\par
00154             {\cf19 if} (parent.isValid())\par
00155                 targetRow = parent.row();\par
00156             {\cf19 else}\par
00157                 targetRow = rowCount();\par
00158         \}\par
00159         \par
00160         {\cf20 // \'C2\'F1\'F2\'E0\'E2\'EB\'FF\'E5\'EC \'ED\'EE\'E2\'FB\'E5 \'F1\'F2\'F0\'EE\'EA\'E8 \'E4\'EB\'FF \'E4\'E0\'ED\'ED\'FB\'F5, \'E5\'F1\'EB\'E8 \'ED\'F3\'E6\'ED\'EE}\par
00161         {\cf18 int} requiredRows = targetRow + cellData.size();\par
00162         {\cf19 while} (rowCount() < requiredRows) \{\par
00163             EquipmentRecord newRecord;\par
00164             newRecord.id = generateUniqueId();\par
00165             addRecord(newRecord);\par
00166         \}\par
00167         \par
00168         {\cf20 // \'CA\'EE\'EF\'E8\'F0\'F3\'E5\'EC \'E4\'E0\'ED\'ED\'FB\'E5}\par
00169         {\cf19 if} (!cellData.isEmpty()) \{\par
00170             {\cf18 int} firstRowKey = cellData.firstKey();\par
00171             \par
00172             {\cf19 for} ({\cf17 auto} rowIt = cellData.begin(); rowIt != cellData.end(); ++rowIt) \{\par
00173                 {\cf18 int} currentRow = targetRow + (rowIt.key() - firstRowKey);\par
00174                 QMap<int, QVariant> columnData = rowIt.value();\par
00175                 \par
00176                 {\cf19 if} (!columnData.isEmpty()) \{\par
00177                     {\cf18 int} firstColKey = columnData.firstKey();\par
00178                     \par
00179                     {\cf19 for} ({\cf17 auto} colIt = columnData.begin(); colIt != columnData.end(); ++colIt) \{\par
00180                         {\cf18 int} targetCol = column >= 0 ? column + (colIt.key() - firstColKey) : colIt.key();\par
00181                         setData(index(currentRow, targetCol), colIt.value());\par
00182                     \}\par
00183                 \}\par
00184             \}\par
00185         \}\par
00186         \par
00187         {\cf19 return} {\cf17 true};\par
00188     \}\par
00189     \par
00190     {\cf20 // \'C8\'EC\'EF\'EE\'F0\'F2 \'E8\'E7 CSV}\par
00191     {\cf19 if} (data->hasFormat({\cf22 "text/csv"}) || data->hasFormat({\cf22 "text/plain"})) \{\par
00192         QString csvData = QString::fromUtf8(data->data(\par
00193             data->hasFormat({\cf22 "text/csv"}) ? {\cf22 "text/csv"} : {\cf22 "text/plain"}));\par
00194             \par
00195         {\cf18 int} targetRow = row;\par
00196         {\cf19 if} (targetRow < 0) \{\par
00197             {\cf19 if} (parent.isValid())\par
00198                 targetRow = parent.row();\par
00199             {\cf19 else}\par
00200                 targetRow = rowCount();\par
00201         \}\par
00202         \par
00203         {\cf18 int} targetCol = column;\par
00204         {\cf19 if} (targetCol < 0 && parent.isValid()) \{\par
00205             targetCol = parent.column();\par
00206         \}\par
00207         \par
00208         {\cf19 return} importFromCSV(csvData, targetRow, targetCol);\par
00209     \}\par
00210     \par
00211     {\cf19 return} {\cf17 false};\par
00212 \}\par
00213 \par
00214 QString EquipmentModel::exportToCSV({\cf17 const} QModelIndexList &indexes){\cf17  const }\{\par
00215     {\cf20 // \'CD\'E0\'F5\'EE\'E4\'E8\'EC \'E3\'F0\'E0\'ED\'E8\'F6\'FB \'E2\'FB\'E4\'E5\'EB\'E5\'ED\'ED\'EE\'E9 \'EE\'E1\'EB\'E0\'F1\'F2\'E8}\par
00216     {\cf18 int} minRow = -1, maxRow = -1, minCol = -1, maxCol = -1;\par
00217     \par
00218     {\cf19 foreach} (QModelIndex index, indexes) \{\par
00219         {\cf19 if} (!index.isValid())\par
00220             {\cf19 continue};\par
00221             \par
00222         {\cf19 if} (minRow == -1 || index.row() < minRow)\par
00223             minRow = index.row();\par
00224             \par
00225         {\cf19 if} (maxRow == -1 || index.row() > maxRow)\par
00226             maxRow = index.row();\par
00227             \par
00228         {\cf19 if} (minCol == -1 || index.column() < minCol)\par
00229             minCol = index.column();\par
00230             \par
00231         {\cf19 if} (maxCol == -1 || index.column() > maxCol)\par
00232             maxCol = index.column();\par
00233     \}\par
00234     \par
00235     {\cf19 if} (minRow == -1 || maxRow == -1 || minCol == -1 || maxCol == -1)\par
00236         {\cf19 return} QString();\par
00237         \par
00238     {\cf20 // \'D1\'EE\'E7\'E4\'E0\'E5\'EC CSV \'E8\'E7 \'E2\'FB\'E4\'E5\'EB\'E5\'ED\'ED\'FB\'F5 \'FF\'F7\'E5\'E5\'EA}\par
00239     QString csv;\par
00240     QTextStream stream(&csv, QIODevice::WriteOnly);\par
00241     \par
00242     {\cf19 for} ({\cf18 int} row = minRow; row <= maxRow; ++row) \{\par
00243         QStringList rowData;\par
00244         \par
00245         {\cf19 for} ({\cf18 int} col = minCol; col <= maxCol; ++col) \{\par
00246             QModelIndex idx = index(row, col);\par
00247             QVariant value = data(idx, Qt::DisplayRole);\par
00248             \par
00249             {\cf20 // \'DD\'EA\'F0\'E0\'ED\'E8\'F0\'F3\'E5\'EC \'E4\'E2\'EE\'E9\'ED\'FB\'E5 \'EA\'E0\'E2\'FB\'F7\'EA\'E8 \'E8 \'EE\'E1\'F0\'E0\'EC\'EB\'FF\'E5\'EC \'EF\'EE\'EB\'E5 \'EA\'E0\'E2\'FB\'F7\'EA\'E0\'EC\'E8 \'E5\'F1\'EB\'E8 \'E5\'F1\'F2\'FC \'E7\'E0\'EF\'FF\'F2\'FB\'E5}\par
00250             QString cellData = value.toString();\par
00251             {\cf19 if} (cellData.contains({\cf23 '"'}))\par
00252                 cellData.replace({\cf22 "\\""}, {\cf22 "\\"\\""});\par
00253                 \par
00254             {\cf19 if} (cellData.contains({\cf23 ','}) || cellData.contains({\cf23 '\\n'}) || cellData.contains({\cf23 '"'}))\par
00255                 cellData = {\cf22 "\\""} + cellData + {\cf22 "\\""};\par
00256                 \par
00257             rowData << cellData;\par
00258         \}\par
00259         \par
00260         stream << rowData.join({\cf22 ","}) << {\cf22 "\\n"};\par
00261     \}\par
00262     \par
00263     {\cf19 return} csv;\par
00264 \}\par
00265 \par
00266 {\cf18 bool} EquipmentModel::importFromCSV({\cf17 const} QString &csvData, {\cf18 int} targetRow, {\cf18 int} targetCol) \{\par
00267     {\cf19 if} (csvData.isEmpty())\par
00268         {\cf19 return} {\cf17 false};\par
00269         \par
00270     QStringList rows = csvData.split({\cf23 '\\n'}, Qt::SkipEmptyParts);\par
00271     {\cf19 if} (rows.isEmpty())\par
00272         {\cf19 return} {\cf17 false};\par
00273         \par
00274     {\cf20 // \'CF\'F0\'EE\'E2\'E5\'F0\'FF\'E5\'EC, \'ED\'F3\'E6\'ED\'EE \'EB\'E8 \'E4\'EE\'E1\'E0\'E2\'E8\'F2\'FC \'ED\'EE\'E2\'FB\'E5 \'F1\'F2\'F0\'EE\'EA\'E8}\par
00275     {\cf18 int} requiredRows = targetRow + rows.size();\par
00276     {\cf19 while} (rowCount() < requiredRows) \{\par
00277         EquipmentRecord newRecord;\par
00278         newRecord.id = generateUniqueId();\par
00279         addRecord(newRecord);\par
00280     \}\par
00281     \par
00282     {\cf20 // \'CF\'E0\'F0\'F1\'E8\'EC \'E8 \'E2\'F1\'F2\'E0\'E2\'EB\'FF\'E5\'EC \'E4\'E0\'ED\'ED\'FB\'E5 \'E8\'E7 CSV}\par
00283     {\cf18 int} currentRow = targetRow;\par
00284     \par
00285     {\cf19 foreach} ({\cf17 const} QString &row, rows) \{\par
00286         {\cf20 // \'CF\'F0\'EE\'F1\'F2\'EE\'E5 \'F0\'E0\'E7\'E4\'E5\'EB\'E5\'ED\'E8\'E5 \'EF\'EE \'E7\'E0\'EF\'FF\'F2\'FB\'EC (\'F3\'EB\'F3\'F7\'F8\'E5\'ED\'ED\'FB\'E9 \'EF\'E0\'F0\'F1\'E5\'F0 \'F3\'F7\'F2\'E5\'F2 \'EA\'E0\'E2\'FB\'F7\'EA\'E8 \'E8 \'FD\'EA\'F0\'E0\'ED\'E8\'F0\'EE\'E2\'E0\'ED\'E8\'E5)}\par
00287         QStringList fields = parseCSVRow(row);\par
00288         \par
00289         {\cf18 int} currentCol = targetCol;\par
00290         {\cf19 foreach} ({\cf17 const} QString &field, fields) \{\par
00291             {\cf19 if} (currentCol < columnCount()) \{\par
