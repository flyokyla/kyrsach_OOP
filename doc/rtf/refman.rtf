00766                 {\cf19 if} (data.contains({\cf23 ','}) || data.contains({\cf23 '"'}) || data.contains({\cf23 '\\n'})) \{\par
00767                     data.replace({\cf22 "\\""}, {\cf22 "\\"\\""});\par
