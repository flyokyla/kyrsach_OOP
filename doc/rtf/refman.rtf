00012 \par
00013 chartwindow::chartwindow({\cf17 const} QVector<EquipmentRecord> &records, QWidget *parent) :\par
00014     QDialog(parent),\par
00015     ui(new Ui::chartwindow),\par
00016     m_records(records)\par
00017 \{\par
00018     ui->setupUi({\cf17 this});\par
00019 \par
00020     ui->comboBox->addItem(tr({\cf22 "Equipment by Type"}));\par
00021     ui->comboBox->addItem(tr({\cf22 "Price Distribution"}));\par
00022     ui->comboBox->addItem(tr({\cf22 "Location Statistics"}));\par
00023 \par
00024     updateChart(0);\par
00025 \}\par
00026 \par
00027 chartwindow::~chartwindow() \{\par
00028     {\cf17 delete} ui;\par
00029 \}\par
00030 \par
00031 {\cf18 void} chartwindow::on_comboBox_currentIndexChanged({\cf18 int} index)\par
00032 \{\par
00033     updateChart(index);\par
00034 \}\par
00035 \par
00036 {\cf18 void} chartwindow::updateChart({\cf18 int} type)\par
00037 \{\par
00038     QChart *chart = {\cf17 new} QChart();\par
00039 \par
00040     {\cf19 if} (type == 0) \{ {\cf20 // Equipment by Type}\par
00041         QMap<QString, int> typeCount;\par
00042         {\cf19 for} ({\cf17 const} EquipmentRecord& eq : m_records) \{\par
00043             typeCount[eq.type]++;\par
00044         \}\par
00045 \par
00046         QBarSeries *series = {\cf17 new} QBarSeries();\par
00047         QBarSet *set = {\cf17 new} QBarSet(tr({\cf22 "Equipment Count"}));\par
00048 \par
00049         QStringList categories;\par
00050         {\cf19 for} ({\cf17 auto} it = typeCount.begin(); it != typeCount.end(); ++it) \{\par
00051             *set << it.value();\par
00052             categories << it.key();\par
00053         \}\par
00054 \par
00055         series->append(set);\par
00056         chart->addSeries(series);\par
00057 \par
00058         QBarCategoryAxis *axisX = {\cf17 new} QBarCategoryAxis();\par
00059         axisX->append(categories);\par
00060         chart->addAxis(axisX, Qt::AlignBottom);\par
00061         series->attachAxis(axisX);\par
00062 \par
00063         QValueAxis *axisY = {\cf17 new} QValueAxis();\par
00064         chart->addAxis(axisY, Qt::AlignLeft);\par
00065         series->attachAxis(axisY);\par
00066 \par
00067         chart->setTitle(tr({\cf22 "Equipment Count by Type"}));\par
00068     \}\par
00069     {\cf19 else} {\cf19 if} (type == 1) \{ {\cf20 // Price Distribution}\par
00070         QBarSeries *series = {\cf17 new} QBarSeries();\par
00071         QBarSet *set = {\cf17 new} QBarSet(tr({\cf22 "Price"}));\par
00072 \par
00073         QStringList categories;\par
00074         {\cf19 for} ({\cf17 const} EquipmentRecord& eq : m_records) \{\par
00075             *set << eq.price;\par
00076             categories << eq.model;\par
00077         \}\par
00078 \par
00079         series->append(set);\par
00080         chart->addSeries(series);\par
00081 \par
00082         QBarCategoryAxis *axisX = {\cf17 new} QBarCategoryAxis();\par
00083         axisX->append(categories);\par
00084         chart->addAxis(axisX, Qt::AlignBottom);\par
00085         series->attachAxis(axisX);\par
00086 \par
00087         QValueAxis *axisY = {\cf17 new} QValueAxis();\par
00088         chart->addAxis(axisY, Qt::AlignLeft);\par
00089         series->attachAxis(axisY);\par
00090 \par
00091         chart->setTitle(tr({\cf22 "Price Distribution by Equipment"}));\par
00092     \}\par
00093     {\cf19 else} \{ {\cf20 // Location Statistics}\par
00094         QMap<QString, int> locationCount;\par
00095         {\cf19 for} ({\cf17 const} EquipmentRecord& eq : m_records) \{\par
00096             locationCount[eq.location]++;\par
00097         \}\par
00098 \par
00099         QPieSeries *series = {\cf17 new} QPieSeries();\par
00100         {\cf19 for} ({\cf17 auto} it = locationCount.begin(); it != locationCount.end(); ++it) \{\par
00101             series->append(it.key(), it.value());\par
00102         \}\par
00103 \par
00104         chart->addSeries(series);\par
00105         chart->setTitle(tr({\cf22 "Equipment Distribution by Location"}));\par
00106     \}\par
00107 \par
00108     chart->legend()->setVisible({\cf17 true});\par
00109     chart->legend()->setAlignment(Qt::AlignBottom);\par
00110 \par
00111     ui->chartView->setChart(chart);\par
00112     ui->chartView->setRenderHint(QPainter::Antialiasing);\par
00113 \}\par
}
