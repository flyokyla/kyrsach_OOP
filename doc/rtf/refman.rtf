00043 \par
00044 {\cf18 void} chartwindow::updateChart({\cf18 int} type)\par
00045 \{\par
00046     QChart *chart = {\cf17 new} QChart();\par
00047 \par
00048     {\cf19 if} (type == 0) \{ {\cf20 // Equipment by Type}\par
00049         QMap<QString, int> typeCount;\par
00050         {\cf19 for} ({\cf17 const} EquipmentRecord& eq : m_records) \{\par
00051             typeCount[eq.type]++;\par
00052         \}\par
00053 \par
00054         QBarSeries *series = {\cf17 new} QBarSeries();\par
00055         QBarSet *set = {\cf17 new} QBarSet(tr({\cf22 "Equipment Count"}));\par
00056 \par
00057         QStringList categories;\par
00058         {\cf19 for} ({\cf17 auto} it = typeCount.begin(); it != typeCount.end(); ++it) \{\par
00059             *set << it.value();\par
00060             categories << it.key();\par
00061         \}\par
00062 \par
00063         series->append(set);\par
00064         chart->addSeries(series);\par
00065 \par
00066         QBarCategoryAxis *axisX = {\cf17 new} QBarCategoryAxis();\par
00067         axisX->append(categories);\par
00068         chart->addAxis(axisX, Qt::AlignBottom);\par
00069         series->attachAxis(axisX);\par
00070 \par
00071         QValueAxis *axisY = {\cf17 new} QValueAxis();\par
00072         chart->addAxis(axisY, Qt::AlignLeft);\par
00073         series->attachAxis(axisY);\par
00074 \par
00075         chart->setTitle(tr({\cf22 "Equipment Count by Type"}));\par
00076     \}\par
00077     {\cf19 else} {\cf19 if} (type == 1) \{ {\cf20 // Price Distribution}\par
00078         QBarSeries *series = {\cf17 new} QBarSeries();\par
00079         QBarSet *set = {\cf17 new} QBarSet(tr({\cf22 "Price"}));\par
00080 \par
00081         QStringList categories;\par
00082         {\cf19 for} ({\cf17 const} EquipmentRecord& eq : m_records) \{\par
00083             *set << eq.price;\par
00084             categories << eq.model;\par
00085         \}\par
00086 \par
00087         series->append(set);\par
00088         chart->addSeries(series);\par
00089 \par
00090         QBarCategoryAxis *axisX = {\cf17 new} QBarCategoryAxis();\par
00091         axisX->append(categories);\par
00092         chart->addAxis(axisX, Qt::AlignBottom);\par
00093         series->attachAxis(axisX);\par
00094 \par
00095         QValueAxis *axisY = {\cf17 new} QValueAxis();\par
00096         chart->addAxis(axisY, Qt::AlignLeft);\par
00097         series->attachAxis(axisY);\par
00098 \par
00099         chart->setTitle(tr({\cf22 "Price Distribution by Equipment"}));\par
00100     \}\par
00101     {\cf19 else} \{ {\cf20 // Location Statistics}\par
00102         QMap<QString, int> locationCount;\par
00103         {\cf19 for} ({\cf17 const} EquipmentRecord& eq : m_records) \{\par
00104             locationCount[eq.location]++;\par
00105         \}\par
00106 \par
00107         QPieSeries *series = {\cf17 new} QPieSeries();\par
00108         {\cf19 for} ({\cf17 auto} it = locationCount.begin(); it != locationCount.end(); ++it) \{\par
00109             series->append(it.key(), it.value());\par
00110         \}\par
00111 \par
00112         chart->addSeries(series);\par
00113         chart->setTitle(tr({\cf22 "Equipment Distribution by Location"}));\par
00114     \}\par
00115 \par
00116     chart->legend()->setVisible({\cf17 true});\par
00117     chart->legend()->setAlignment(Qt::AlignBottom);\par
00118 \par
00119     ui->chartView->setChart(chart);\par
00120     ui->chartView->setRenderHint(QPainter::Antialiasing);\par
00121 \}\par
}
